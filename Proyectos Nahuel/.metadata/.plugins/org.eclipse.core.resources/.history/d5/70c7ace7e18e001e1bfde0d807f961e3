package tema5;

public class Persona {
	
	//Atributos (o propiedades)
	String nombre;
	byte edad= 20;
	double altura= 1.72D;
	
	//Metodos
	public void saludar() {
		System.out.println("hola");
	}
	public void caminar() {
		System.out.println("Estoy caminando");
	}
	
	public void cumple() {
		edad ++;
	}
	
}
