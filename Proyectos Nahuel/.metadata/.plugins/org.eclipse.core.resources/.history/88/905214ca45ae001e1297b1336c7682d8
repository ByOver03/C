package herencia1;

class Persona {
	//Atributos
	String nombre;
	int edad;
	double estatura;
	
}

class Empleado extends Persona{
	double salario;
	boolean empleadodelmes;
	Empleado(String nombre, int edad, double estatura,double salario, boolean empleadodelmes){
		this.salario= salario;
		this.empleadodelmes=empleadodelmes;
		this.nombre= nombre;
		this.edad= edad;
		this.estatura= estatura;
	}
}