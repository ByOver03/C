<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="vehiculo">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="barco"></xs:enumeration>
                <xs:enumeration value="bicicleta"></xs:enumeration>
                <xs:enumeration value="coche"></xs:enumeration>
                <xs:enumeration value="tren"></xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
</xs:schema>